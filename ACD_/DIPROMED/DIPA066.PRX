Static Function VldRegras(cArmazem,cLocaliz)
Local lRet       := .T.
Local cSQL       := ""
Local nContMax   := 0
Local cZeraRua   := .F.
DEFAULT cArmazem := ""
DEFAULT cLocaliz := ""


//Endereço bloqueado - STATUS = 3
If SBE->BE_STATUS == '3'
	VTAlert("Endereço "+AllTrim(SBE->BE_LOCALIZ)+" não precisa de inventário. Vá para o próximo endereço.","Atenção",.T.,4000,3)
	lRet := .F.
EndIf

//Se já foi feita pelo menos uma contagem, começo as verificações
If lRet .And. SBE->BE_XNRCONT > 0
	//Acho a contagem atual (max)
	cSQL := " SELECT " 
	cSQL += " 	MAX(BE_XNRCONT) CONTAGEM "
	cSQL += " 	FROM " 
	cSQL +=  		RetSQLName("SBE")
	cSQL += " 		WHERE "
	cSQL += " 			BE_FILIAL = '04' AND "
	cSQL += " 			BE_LOCAL = '"+cArmazem+"' AND "
	cSQL += " 			BE_STATUS <> '3' AND "				
	cSQL += " 			BE_PRIOR NOT IN(' ','ZZZ') AND "
	cSQL += " 			D_E_L_E_T_ = ' '  "
	
	cSQL := ChangeQuery(cSQL)
	dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSQL),"QRYMAX",.T.,.T.)
	
	If !QRYMAX->(Eof())
		nContMax := QRYMAX->CONTAGEM
	EndIf
	QRYMAX->(dbCloseArea())

	//Verifico se existem endereços pendentes na contagem anterior ou se posso liberar uma nova contagem
	If SBE->BE_XNRCONT >= nContMax
		cSQL := " SELECT " 
		cSQL += " 	BE_LOCALIZ "
		cSQL += " 	FROM " 
		cSQL +=  		RetSQLName("SBE")
		cSQL += " 		WHERE "
		cSQL += " 			BE_FILIAL = '04' AND "
		cSQL += " 			BE_LOCAL = '"+cArmazem+"' AND "
		cSQL += " 			BE_STATUS <> '3' AND "
		cSQL += " 			BE_XDATCON = ' ' AND "			
		cSQL += " 			BE_PRIOR NOT IN(' ','ZZZ') AND "
		cSQL += " 			BE_XNRCONT < "+StrZero(nContMax,1)+" AND "			
		cSQL += " 			D_E_L_E_T_ = ' '  "
		cSQL += " ORDER BY BE_LOCALIZ "
	
		cSQL := ChangeQuery(cSQL)
		dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSQL),"QRYCON",.T.,.T.)
		
		If QRYCON->(Eof())
			//Se não existir, habilito uma nova contagem (apaga dados da contagem anterior)
			VTAlert("Parabéns! Você finalizou uma contagem completa no estoque DIPROMED. Reiniciando endereços para nova contagem.","Atenção",.T.,4000,3)
			DipDelRot()						
		Else
			//Se existir, não libera o endereço informado.
			VTAlert("Endereço "+AllTrim(SBE->BE_LOCALIZ)+" já contado anteriormente.","Atenção",.T.,4000,3)
			VTAlert("Vá para o endereço: "+AllTrim(QRYCON->BE_LOCALIZ),"Atenção",.T.,4000,3)
			lRet := .F.	

		EndIf
		QRYCON->(dbCloseArea())		
	EndIf
EndIf

//Agora vou verificar se existe rua pendente para o usuário.
If lRet
	If !Empty(SBE->BE_XUSUROT) .And. SBE->BE_XUSUROT <> Upper(AllTrim(U_DipUsr()))
		VTAlert("O usuário "+SBE->BE_XUSUROT+" já iniciou a contagem neste endereço.","Atenção",.T.,4000,3)
		lRet := .F.
	Else
		cSQL := " SELECT " 
		cSQL += " 	BE_LOCALIZ " 
		cSQL += " 	FROM " 
		cSQL +=  		RetSQLName("SBE")
		cSQL += " 		WHERE "
		cSQL += " 			BE_FILIAL  = '04' AND "
		cSQL += " 			BE_LOCAL   = '"+cArmazem+"' AND "
		cSQL += " 			BE_STATUS <> '3' AND "
		cSQL += " 			BE_XUSUROT = '"+Upper(AllTrim(U_DipUsr()))+"' AND "
		cSQL += " 			BE_PRIOR NOT IN(' ','ZZZ') AND "
		cSQL += " 			BE_XDATCON = ' ' AND "			
		cSQL += " 			D_E_L_E_T_ = ' ' "
		cSQL += " ORDER BY BE_LOCALIZ "  
		
		cSQL := ChangeQuery(cSQL)
		dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSQL),"QRYSBE",.T.,.T.)
		
		If !QRYSBE->(Eof())
			if Left(cLocaliz,2)<>Left(QRYSBE->BE_LOCALIZ,2)
				VTAlert("Falta concluir a contagem no lado "+Left(QRYSBE->BE_LOCALIZ,2)+". Vá para o endereço: "+AllTrim(QRYSBE->BE_LOCALIZ)+".","Atenção",.T.,4000,3)
				lRet := .F.
			EndIf
		/*Else //Desativado em 23/02/2021 Maximo
			If cZeraRua
				DipGrvRua(cLocaliz)*/
		EndIf
		QRYSBE->(dbCloseArea())
	EndIf
EndIf

Return lRet
