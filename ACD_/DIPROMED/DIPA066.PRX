Static Function DipDelRot()
Local aAreaSBE := SBE->(GetArea())
Local cSQL := ""

//Apaga as informações do inventário para iniciar uma nova contagem
cSQL := " SELECT " 
cSQL += " 	R_E_C_N_O_ REC " 
cSQL += " 	FROM " 
cSQL +=  		RetSQLName("SBE")
cSQL += " 		WHERE "
cSQL += " 			BE_FILIAL  = '04' AND "
cSQL += " 			BE_LOCAL   = '"+cArmazem+"' AND "
cSQL += " 			BE_STATUS <> '3' AND "			
cSQL += " 			BE_PRIOR NOT IN(' ','ZZZ') AND "
cSQL += " 			D_E_L_E_T_ = ' ' " 

cSQL := ChangeQuery(cSQL)
dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSQL),"GRVDEL",.T.,.T.)

While !GRVDEL->(Eof())
	SBE->(dbGoTo(GRVDEL->REC))
	SBE->(RecLock("SBE",.F.))
		SBE->BE_XUSUROT := ""
		SBE->BE_XDATCON := STOD(' ')	
		If SBE->BE_XNRCONT == 9
			SBE->BE_XNRCONT := 0
		EndIf	
	SBE->(MsUnLock())
	GRVDEL->(dbSkip())
EndDo
GRVDEL->(dbCloseArea())

RestArea(aAreaSBE)
	
Return 
