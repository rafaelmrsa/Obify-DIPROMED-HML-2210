Static Function Contagem()
Local nDipQtd   := 0
Local cProduto  := Space(25)
Local cPicQtdExp:= PesqPict("CB8","CB8_QTDORI")
Local lSai 		:= .F.

ZZC->(RecLock("ZZC",.F.))
	ZZC->ZZC_STATUS := "1"
ZZC->(MsUnLock())

aTela := VtSave()
VtClear()              

While !lSai

	nDipQtd   := 0
	cProduto  := Space(25)
	
	@ 0,0 VTSay "Leia o Produto"
	@ 2,0 VTSay "Quantidade:" 
	@ 3,0 VtGet nDipQtd pict cPicQtdExp
	@ 4,0 VTSay "Produto"
	@ 5,0 VTGet cProduto pict '@!' VALID VTLastkey() == 5 .or. VldPrd066(cProduto,nDipQtd)
	
	VTRead
	
	If VtLastKey() == 27
		If VTYesNo("Deseja finalizar o inventário do endereço "+cArmazem+"-"+cLocaliz+"?","Atencao",.T.)
			If Dip66Ok()         
				VTAlert("Inventário SEM divergências. Vá para o próximo endereço.","Atenção",.T.,7000,3)		
				ZZC->(RecLock("ZZC",.F.))
					u_DipBlqEnd(ZZC->ZZC_LOCAL,ZZC->ZZC_LOCALI,StoD(""))
					ZZC->ZZC_STATUS := "4"
				ZZC->(MsUnLock())							
			Else
				VTAlert("Inventário COM divergências.","Atenção",.T.,7000,3)		
				ZZC->(RecLock("ZZC",.F.))
					ZZC->ZZC_STATUS := "2"
				ZZC->(MsUnLock())
			EndIf
			
			lSai := .T.
		ElseIf VTYesNo("Deseja sair?","Atencao",.T.)
			ZZC->(RecLock("ZZC",.F.))
				ZZC->ZZC_STATUS := "3"
			ZZC->(MsUnLock())
			lSai := .T.
		Endif
	EndIf
EndDo
	
Return
