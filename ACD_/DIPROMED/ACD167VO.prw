User Function DipVldVol(cVolume,cOrdSep)
Local lRet := .F.                     
Local cSQL := ""

cSQL := "SELECT "
cSQL += "	CB9_ORDSEP " 
cSQL += "	FROM "
cSQL += 		RetSQLName("CB9")
cSQL += "		WHERE "
cSQL += "			CB9_FILIAL = '"+xFilial("CB9")+"' "
cSQL += "			AND CB9_ORDSEP = '"+cOrdSep+"' "
cSQL += "			AND CB9_VOLUME = '"+cVolume+"' "
cSQL += "			AND D_E_L_E_T_ = ' ' "          

cSQL := ChangeQuery(cSQL)
dbUseArea(.T.,"TOPCONN",TcGenQry(,,cSQL),"QRYVOL",.T.,.T.)

lRet := QRYVOL->(Eof())
QRYVOL->(dbCloseArea())	

Return lRet