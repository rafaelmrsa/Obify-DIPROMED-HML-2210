User Function FT210MNU()
*--------------------------------------------------------------------------*

Local cUseAuto := GetMv("ES_FT210_1",,"GIO100/MCANUTO")
aRotina:={}
aadd(aRotina,{ "Pesquisar","AxPesqui"   ,0,1,0,.F.	})  // Pesquisar
aadd(aRotina,{ "Visualizar","U_Ft210Visu" ,0,2,0,NIL	})  // Visual
