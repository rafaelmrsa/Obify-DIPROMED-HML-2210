	aadd(aRotina,{ "Liberar","Ft210Liber" ,0,4,0,NIL	})	// "Liberar"
EndIf
aadd(aRotina,{ "Legenda","Ft210Legend",0,2,0,.F.	})	// "Legenda"

Return(aRotina)

