User Function MA410MNU()    

If Type("l410Auto")<>"U" .And. l410Auto
	Return
EndIf


If aScan(aRotina, {|x| x[2]=="A410Altera"}) > 0

	aRotina[4,2] := "u_Dip410Alt()"
		
Endif

If aScan(aRotina, {|x| x[2]=="A410Inclui"}) > 0

	aRotina[3,2] := "u_Dip410Inc()"
		
EndIf



If UPPER(U_DipUsr())$GetNewPar("ES_DESBEST","DDOMINGOS/MCANUTO/VEGON/RBORGES/GNUNES/EMATIAS/SFONSECA")
	Aadd(aRotina,{ "Desb.Estorno" ,"u_DipDeEst",0,0,0 ,NIL} )
EndIf                                          

Aadd(aRotina,{ "Avalia Aprova็ใo","u_VldAprDig",0,0,0 ,NIL} )

Aadd(aRotina,{ "Valida Reserva","u_DipSolApr",0,0,0 ,NIL} )

If UPPER(U_DipUsr())$GetNewPar("ES_SOLAPRO","/RBORGES/RLOPES/ASOUZA/ACASAGRANDE/CSANTOS/EMASCALCHI/ERODRIGUES/JCERQUEIRA/KSANTOS/KRODRIGUES/LAGUIAR/MORNELLAS/MCOSTA/RAPARECIDA/TUTSUMI/ABARRETO/ALOPES/BRODRIGUES/CPEREIRA/DNASCIMENTO/MCANUTO/VSOUSA/BMESQUITA/IANJOS")
	Aadd(aRotina,{ "Solicita Avalia็ใo","U_DIPDIG01(,,'A')",0,0,0 ,NIL} )
EndIf

Return                            
