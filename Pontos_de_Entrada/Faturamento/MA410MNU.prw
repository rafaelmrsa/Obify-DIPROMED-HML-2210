User Function DipDeEst()

If Empty(SC5->C5_XBLQNF)
	Aviso("Aten็ใo","Pedido jแ desbloqueado.",{"Ok"},1)
Else         
	SC5->(RecLock("SC5",.F.))
		SC5->C5_XBLQNF := ""
	SC5->(MsUnLock())
	Aviso("Aten็ใo","Pedido desbloqueado com sucesso.",{"Ok"},1)
	Conout("Pedido "+SC5->C5_NUM+" desbloqueado pelo usuแrio: "+U_DipUsr()+" ("+DtoC(dDataBase)+" - "+Time()+")")
EndIf

Return
